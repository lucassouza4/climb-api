generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Boulder {
  id         Int    @id @default(autoincrement())
  key        String @unique
  name       String
  difficulty Int
  sector     String
  city       String
  ascensions Int

  @@map("Boulders")
}
