{
  "name": "climb-api",
  "version": "1.0.0",
  "description": "API in Node.js and Typescript using Docker and Clean Architecture",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.ts",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "husky:prepare": "husky",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "eslint": "^9.9.0",
    "git-commit-msg-linter": "^5.0.8",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  }
}
